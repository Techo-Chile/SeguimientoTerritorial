<h1>Listado de Comunidades</h1>
<table>
  <tr>
    <th>Nombre</th>
    <th>País</th>
    <th>Región</th>
    <th>Comuna</th>
    <th>Estado</th>
    <th colspan="3"></th>
  </tr>

  <% @communities.each do |community| %>
    <tr>
      <th><%= link_to community.community_name, community_path(community) %></th>
      <th><%= community.country%></th>
      <th><%= community.province_region%></th>
      <th><%= community.municipality_location%></th>
      <th><%= community.community_status%></th>
    </tr>
  <% end %>
</table>
